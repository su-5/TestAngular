<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="shortcut icon" type="image/x-icon" href="~/favicon.ico" />
    <title>@ViewBag.Title</title>
    @Scripts.Render("~/bundles/scripts", "~/Angular/main.js")
    @Styles.Render("~/Content/css")
</head>
<body ng-app="Web">
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div align="center" class="container">
            <h3 class="titleColor">Музыкальный каталог</h3>
        </div>
    </div>
    <div class="msg-overlay" ng-show="loadingIsShow">
        <div class="msg">
            <span>
                Обработка данных...
                <i class="fa fa-refresh fa-spin fa-fw"></i>
            </span>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />

    </div>
    <footer class="footer-setting">
        <p>&copy; @DateTime.Now.Year - Проект Янченко Андрея Анатольевича</p>
    </footer>
    @RenderSection("scripts", required: false)
</body>
</html>